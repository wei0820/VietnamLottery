<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.jingyu.pay.ui.home.HomeFragment"
        android:label="买币"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.jingyu.pay.ui.dashboard.DashboardFragment"
        android:label="卖币"
        tools:layout="@layout/fragment_dashboard" />
    <fragment
        android:id="@+id/navigation_order"
        android:name="com.jingyu.pay.ui.order.OrderFragment"
        android:label="进行中订单"
        tools:layout="@layout/fragment_order" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.jingyu.pay.ui.notifications.NotificationsFragment"
        android:label="个人"
        tools:layout="@layout/fragment_notifications" />
</navigation>